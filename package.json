{
  "name": "@ylveracode/veracode-cli",
  "version": "0.1.1",
  "description": "a NodeJS based API wrapper for utilizing the Veracode APIs",
  "homepage": "https://lerer.github.io/veracode-cli/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lerer/veracode-cli.git"
  },
  "engines": {
    "node": ">=14"
  },
  "bin": {
    "veracode-cli": "out/veracode-cli.js"
  },
  "scripts": {
    "test-workspaces": "node ./out/test/testAPI.js",
    "clean": "del-cli ./out/*",
    "build": "npm run clean && tsc",
    "pub": "git add . && git commit -m \"Updates\" && npm version patch && npm run build && npm publish"
  },
  "keywords": [
    "cli",
    "veracode",
    "SCA",
    "SAST"
  ],
  "preferGlobal": true,
  "author": "Yaakov Lerer",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "configparser": "^0.3.8",
    "prompts": "^2.4.0",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/prompts": "^2.0.9",
    "@types/yargs": "^16.0.0",
    "del-cli": "^3.0.1"
  },
  "bugs": {
    "url": "https://github.com/Lerer/veracode-cli/issues"
  },
  "main": "./out/veracode-cli.js",
  "directories": {
    "doc": "docs"
  }
}
